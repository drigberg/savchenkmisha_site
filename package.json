{
  "name": "savchenkmisha_site",
  "version": "1.0.0",
  "description": "personal website for a dear friend",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel api:start ui:start",
    "start:dev": "npm-run-all --parallel api:start:dev ui:start",
    "test": "npm run api:test",
    "build": "npm run --prefix ui build",
    "postinstall": "cd ui && npm install",
    "api:start": "NODE_ENV=production node api/start.js",
    "api:start:dev": "NODE_ENV=production nodemon api/start.js",
    "api:test": "NODE_ENV=test mocha api/test/setup.js api/test/*.test.js",
    "ui:start": "npm run --prefix ui start",
    "seed-db": "node api/db/seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drigberg/savchenkmisha_site.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/drigberg/savchenkmisha_site/issues"
  },
  "homepage": "https://github.com/drigberg/savchenkmisha_site#readme",
  "dependencies": {
    "body-parser": "1.18.3",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "jsonwebtoken": "8.4.0",
    "npm-run-all": "4.1.5",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "faker": "4.1.0",
    "mocha": "5.2.0",
    "nodemon": "1.18.7",
    "request-promise": "4.2.2",
    "supertest": "3.3.0"
  }
}
